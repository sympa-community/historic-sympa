<?xml version="1.0" encoding="iso-8859-1" ?><!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xml:lang="[% iso639 = locale.match('^(.)(.)'); iso639.0; iso639.1 %]">

<!-- begin main.tt2 (distrib) -->
<head>
<title> [% main_title %] </title>
<link rel="stylesheet" href="[% path_cgi %]/css/style.css" type="text/css" media="screen" title="Derived from style Blank Canvas by tw3k AT tw3k.tw3k.q3machines.net" />
<link rel="stylesheet" href="[% path_cgi %]/css/print.css" type="text/css" media="print" />
<link rel="alternative stylesheet" href="[% path_cgi %]/css/fullPage.css" type="text/css" media="screen" title="View Full Page" />
<link rel="alternative stylesheet" href="[% path_cgi %]/css/print-preview.css" type="text/css" media="screen" title="Print Preview" />
<meta name="generator" content="Sympa" />

[% IF conf.web_recode_to %]
<meta http-equiv="content-type" content="text/html; charset=[% conf.web_recode_to %]" />
[% ELSE %]
<meta http-equiv="content-type" content="text/html; charset=[%|loc%]_charset_[%END%]" />
[% END %]

[% PROCESS javascript.tt2 %]

[% IF base %] <base href="[% base %]" /> [% END %]
<!--[if IE]>
<style>
.ContentBlock {
	margin: -7px 0px 0px 0px;
}
</style>
<![endif]-->
</head>

<body onload="[% IF use_htmlarea %] HTMLArea.replaceAll();[% END %][% IF cookie_set %] check_cookie();[% END %][% IF back_to_mom %] setTimeout('refresh_mom_and_die()',1000);[% END %]">
<noscript><p><em>[%|loc%]Please activate JavaScript in your web browser[%END%]</em></p></noscript>

<div id="Canvas">
  <div id="Header">
    [% PROCESS title.tt2 %]
    [% PROCESS menu.tt2 %]
  </div> <!-- end div Header -->

  <div id="Menus"><!-- Start Menus -->&nbsp;
    [% PROCESS login_menu.tt2 %]
    [% PROCESS list_menu.tt2 IF list %]
  </div><!-- End div Menus -->
  <div id="Stretcher">  <!-- <span id="toggleMenu" title="A javascript could hidde or show the meneu" class="smalltext info printNone toggleNone">toggle menu</span><br /> --> &nbsp;<br />
     <div id="Paint">
       [% PROCESS error.tt2 IF errors %]
       [% PROCESS notice.tt2 IF notices %]
       <div class="ContentBlock"> 
       [% IF list %]<h3 style="text-align: center;">[% list_protected_email %]</h3><br />[% END %]
         [% PROCESS "${action}.tt2" IF action %]
       </div>
       <a class="dingbat printNone" href="#top" title="Top of Page">&sect;</a>
     </div><!-- End div= Paint -->
  </div><!-- end div Stretcher -->
  [% PROCESS footer.tt2 %]

</div> <!-- end div canvas -->

</body>
</html>
