	font-weight: 100;
	font-size: 20px;
}

h4 {
	color: [% color_2 %];
	font-size: 16px;
	margin: 1.3em 0px 0px;
	font-weight: 100;
	text-transform: uppercase;
}

h5 {
	color: [% color_3 %];
	font-size: 14px;
	margin: 2em 0px 0.2em;
	font-weight: 100;
}

h1 a, h2 a, h3 a {
	color:[% color_2 %];
}

/* Tables */

table {
	position:relative;
	left:1px;
	width:99%;
	border:1px solid [% color_9 %];
	border-collapse:none;
	border-spacing: 0;
	border-radius:5px;
}

td {
