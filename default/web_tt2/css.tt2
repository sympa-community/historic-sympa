.nav li.MainMenuLinksCurrentPage {
	font-weight:normal;
	background: none repeat scroll 0 0 [% color_11 %];
	color: [% color_7 %];
	padding-top: 0.2em;
}

.nav li.MainMenuLinksCurrentPage a{
	background-color: transparent;
}

.nav li.nolink{
	background: none repeat scroll 0 0 [% color_11 %];
	padding-top: 0.2em;
