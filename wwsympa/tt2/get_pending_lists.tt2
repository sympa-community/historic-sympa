<!-- $Id$ -->


<FORM ACTION="[% path_cgi %]" METHOD=POST>

<HR  WIDTH=90%>

<P>
<TABLE>
 <TR>
   <TD Colspan=3 bgcolor="[% light_color %]"><B>[%|l%]Pending lists[%END%]</B></TD>
 </TR>
 <TR bgcolor="[% light_color %]">
   <TD><B>[%|l%]list name[%END%]</B></TD>
   <TD><B>[%|l%]list subject[%END%]</B></TD>
   <TD><B>[%|l%]Requested by[%END%]</B></TD>
 </TR>

[% FOREACH list = pending %]
    [% SET tmp = list.key list = list.value list.NAME = tmp IF list.key.defined %]
<TR>
<TD><A HREF="[% path_cgi %]/set_pending_list_request/[% list.NAME %]">[% list.NAME %]</A></TD>
<TD>[% list.subject %]</TD>
<TD>[% list.by %]</TD>
</TR>
[% END %]
</TABLE>
</FORM>



